{
  "version": 2,
  "builds": [
    {
      "src": "script.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/script.js"
    }
  ],
  "functions": {
    "script.js": {
      "includeFiles": ["views/**", "public/**"]
    }
  }
}
